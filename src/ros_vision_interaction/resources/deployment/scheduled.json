                "content": "{'var': 'good response'}",
                "options": ["Next", "Replay"],
                "message_type": "multiple choice one column"
            },
            "fair": {
                "transitions": ["exit", "fair"],
                "content": "{'var': 'fair response'}",
                "options": ["Next", "Replay"],
                "message_type": "multiple choice one column"
